{
  "name": "brrr",
  "version": "0.1.0",
  "description": "Horizontally scalable workflow scheduling with pluggable backends",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "check": "biome check --write && sort-package-json --quiet",
    "test": "tsx --test",
    "test:watch": "tsx --test --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@datastructures-js/deque": "^1.0.5",
    "bencode": "^4.0.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@ai-zen/async-queue": "^1.3.2",
    "@biomejs/biome": "^1.9.4",
    "@types/bencode": "^2.0.4",
    "@types/node": "^22.15.3",
    "sort-package-json": "^3.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
